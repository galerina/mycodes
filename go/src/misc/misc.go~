package misc

func IsMultiple(n, base int) bool {
	return (n%base == 0)
}

func FibList(n int) []int {
	prev := 1
	current := 1
	list := make([]int, 0, n/2)
	for ;current <= n; prev, current = current, current+prev {
		list = append(list, current)
	}

	return list
}

func Max(a,b int) int {
	if a > b {
		return a
	} else {
		return b
	}
}
